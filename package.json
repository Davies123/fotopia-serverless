{
  "name": "fotopia-serverless",
  "version": "1.0.1",
  "description": "Serverless photo web app using Lambda, S3, DynamoDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbudm/fotopia-serverless.git"
  },
  "author": "Steve Roberts <mbudm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "tape './fotos/**/*__test__.js' -r babel-register | tap-dot",
    "test-watch": "tape-watch './fotos/**/*__test__.js' -r babel-register -p tap-spec",
    "functional": "tape ./test/functional.js -r babel-register | tap-spec",
    "functional-local": "IS_OFFLINE=1 tape ./test/functional.js -r babel-register | tap-spec",
    "lint": "eslint fotos"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "aws4": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "dotenv": "^5.0.1",
    "joi": "^13.0.2",
    "leaked-handles": "^5.2.0",
    "serverless-s3-remover": "^0.4.1",
    "serverless-stack-output": "^0.2.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "aws-amplify": "^0.2.11",
    "aws-sdk": "^2.12.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "dynamodb-local": "^0.0.18",
    "es6-promise": "^4.1.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "isomorphic-fetch": "^2.2.1",
    "pre-commit": "^1.2.2",
    "s3-upload-stream": "^1.0.7",
    "serverless-dynamodb-local": "^0.2.18",
    "serverless-offline": "^3.8.3",
    "serverless-s3-local": "^0.3.2",
    "serverless-s3-local-sync": "^1.0.1",
    "serverless-s3-sync": "^1.3.0",
    "serverless-webpack": "^4.1.0",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tape-watch": "^2.3.0",
    "webpack": "^3.9.1",
    "webpack-node-externals": "^1.6.0",
    "zlib": "^1.0.5"
  }
}
